{
    "name": "dzava/global-search",
    "description": "",
    "keywords": [
        "laravel",
        "global",
        "search"
    ],
    "homepage": "https://github.com/dzava/global-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitris Zavantias",
            "email": "dzavantias@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dzava\\GlobalSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzava\\GlobalSearch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dzava\\GlobalSearch\\GlobalSearchServiceProvider"
            ]
        }
    }
}
